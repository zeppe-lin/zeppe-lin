# Description: Property displayer for X
# URL:         http://xorg.freedesktop.org
# Depends on:  xorg-libxmu

name=xorg-xprop
version=1.2.5
release=1
source=https://www.x.org/releases/individual/app/xprop-$version.tar.bz2

build() {
	cd xprop-$version

	./configure --prefix=/usr

	make V=1
	make DESTDIR=$PKG install
}
